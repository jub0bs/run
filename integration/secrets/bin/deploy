#!/bin/bash

PROJECT_ID=$(gcloud config get-value project)

gcloud run deploy run-secrets-integration-tests \
  --allow-unauthenticated \
  --concurrency 10 \
  --cpu 1 \
  --image gcr.io/${PROJECT_ID}/run-secrets-integration-test:0.0.14 \
  --memory '128Mi' \
  --platform managed \
  --region us-west1
